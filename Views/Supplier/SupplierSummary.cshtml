@model Inventory_Management_System.ViewModels.SupplierViewModel;

@{
    Layout = "_Layout";
    ViewData["Title"] = "Supplier Summary";
    var sList = Model.supList;
    var emp = Model.employee;

}


<h1>@ViewData["Title"]</h1>
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Purchase Order Pending Delivery</h4>
            <div class="table-vertical-scroll">
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>Supplier Name</th>
                            <th>Contact Name</th>
                            <th>Phone Number</th>
                            <th>Fax Number</th>
                            <th>Address</th>
                            <th>GST Registration Number</th>

                        </tr>
                    </thead>
                    <tbody>
                        @{
                            foreach (var s in sList)
                            {
                                <tr>
                                    <td>@s.SupplierName</td>
                                    <td>@s.ContactName</td>
                                    <td>@s.PhoneNumber</td>
                                    <td>@s.FaxNumber</td>
                                    <td>@s.Address</td>
                                    <td>@s.GSTregistrationNumber</td>
                                </tr>
                            }
                        }


                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

@section Styles{
    <link rel="stylesheet" href="../../../css/shared/style.css" />
    <link rel="stylesheet" href="../../../css/demo_1/style.css" />
}